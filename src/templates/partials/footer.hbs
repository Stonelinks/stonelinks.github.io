---
component: footer
---

<footer>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
              {{#each site.social_networks}}
                <a class="social" href="{{this.link}}" target="_blank">
                  <i class="fa fa-{{this.name}}"></i>
                </a>
              {{/each}}
              <a class="footer-link" href="{{this.link}}">
                contact
              </a>
            </div>
        </div>
    </div>
</footer>
